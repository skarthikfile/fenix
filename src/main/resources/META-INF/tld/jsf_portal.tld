<!DOCTYPE taglib
  PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
  "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>

    <tlib-version>1.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>fp</short-name>
    <uri>http://fenixedu.org/taglib/jsf-portal</uri>
    <description>
        Integration of JSF with Bennu Portal
    </description>

    <tag>
        <name>select</name>
        <tag-class>org.fenixedu.academic.servlet.taglib.PortalJSFTag</tag-class>
        <description>
            Tag that marks the given @StrutsFunctionality Action as the selected functionality.
        </description>

        <attribute>
            <name>actionClass</name>
            <required>true</required>
        </attribute>
    </tag>


</taglib>
